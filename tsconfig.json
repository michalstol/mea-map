{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noFallthroughCasesInSwitch": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "noEmit": true,
    "jsx": "react-jsx",
    "baseUrl": "src",
    "paths": {
      "__mocks__/*": ["__mocks__/*"],
      "@assets/*": ["@assets/*"],
      "@configs/*": ["@configs/*"],
      "@typings/*": ["@typings/*"],
      "@hooks/*": ["@hooks/*"],
      "@helpers/*": ["@helpers/*"],
      "@pages/*": ["@pages/*"],
      "@redux/*": ["@redux/*"],
      "@styles/*": ["@styles/*"],
      "@ui/*": ["@ui/*"]
    }
  },
  "include": [
    "src"
  ]
}
